{
    "name": "delboy1978uk/bone-i18n",
    "description": "I18n package for Bone Framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Stephen McLean",
            "email": "delboy1978uk@gmail.com"
        }
    ],
    "require": {
        "php": ">7.1",
        "delboy1978uk/barnacle": "^2.3",
        "delboy1978uk/form": "^2.1",
        "laminas/laminas-i18n": "^2.10",
        "laminas/laminas-servicemanager": "^3.3",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "delboy1978uk/bone-http": "^1.2",
        "delboy1978uk/bone-view": "^1.0",
        "delboy1978uk/bone-traits": "^1.0"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "laminas/laminas-diactoros": "2.*",
        "roave\/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Bone\\I18n\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoneTest\\": "tests/unit/"
        }
    }
